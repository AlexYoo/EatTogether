<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.manuel.tacademy.eattogether.good_restaurant.GoodRestFragment">

    <!--tools:listItem : 커스텀 셀 프리뷰-->
    <android.support.v7.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/recyclerView"
        android:layout_marginTop="60dp"
        android:padding="4dp"
        tools:listitem="@layout/cell_main_goodrest_list">

    </android.support.v7.widget.RecyclerView>

    <include layout="@layout/good_rest_top_drawer"/>


    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="@dimen/fab_margin"
        app:fabSize="normal"
        app:backgroundTint="#DB8D0F"
        app:srcCompat="@drawable/google_pin" />

    <include layout="@layout/content_test" />


</FrameLayout>
